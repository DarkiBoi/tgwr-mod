add_namespace = ww1_darfur

# Ali Dinar Dies!
country_event = {
	title = ww1_darfur.1.t
	desc = ww1_darfur.1.d
	id = ww1_darfur.1
	picture = GFX_report_event_royal_parade
	fire_only_once = yes
	mean_time_to_happen = {
		days = 12
	}
	trigger = {
		tag = DAR
		has_country_leader = {
			character = DAR_ali_dinar
			ruling_only = yes
		}
		has_capitulated = yes
	}
	option = {
		name = ww1_darfur.1.a
		retire_character = DAR_ali_dinar
	}
}
